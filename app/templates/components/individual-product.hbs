<div class='col-xs-12 col-sm-6 col-md-4'>
  <div class='thumbnail'>
    <img class='img-responsive' src={{product.imageUrl}}>
    <div class='caption'>
      <h3>{{product.name}}</h3>
      <p>Color: {{product.description}}</p>
      <p>Category: {{product.category}}</p>
      <p>Price {{product.price}}</p>
      <p>Stock: {{product.quantityInStock}}</p>
      <center><input class='btn btn-primary' type='submit' value='Add To Cart'></center>
    </div>
  </div>
</div>
